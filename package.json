{
  "name": "gotymandering",
  "version": "0.0.1",
  "description": "The best way to live-build your outlet's GOTY list.",
  "homepage": "https://github.com/sodagabe/gotymandering#readme",
  "bugs": {
    "url": "https://github.com/sodagabe/gotymandering/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sodagabe/gotymandering.git"
  },
  "license": "ISC",
  "author": "Gabe Tovar",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "prepare": "husky",
    "sass": "sass --style=compressed ./scss/main.scss ./css/main.css",
    "watch": "sass --watch ./scss/main.scss ./css/main.css",
    "prefix": "postcss ./css/main.css --replace --use autoprefixer -b 'defaults' --no-map",
    "compile": "npm run sass && npm run prefix"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "bootstrap": "^5.3.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "3.7.4",
    "sass": "^1.97.2"
  }
}
